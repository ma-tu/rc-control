        <ul class="nav navbar-nav">
            <li class="active"><a href="#tab-rc" data-toggle="pill">ラジコン</a></li>
            <li><a href="#tab-test" data-toggle="pill">テスト</a></li>
        </ul>
    </div>
</nav>

<!-- コンテント -->
<div class="container">
    <div id="app-content" class="tab-content">
        <div class="tab-pane fade in active" id="tab-rc">
            <% include partial/partial-rc %>
        </div>
        <div class="tab-pane fade" id="tab-test">
            <% include partial/partial-test %>
        </div>
